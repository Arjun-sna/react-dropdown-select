{
  "name": "react-dropdown-select",
  "version": "1.3.4",
  "description": "Customizable dropdown select for react",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "start": "webpack-dev-server --mode development",
    "transpile": "BABEL_ENV=production rm -rf ./dist && babel src -d dist",
    "prepublishOnly": "npm run transpile",
    "build": "webpack --mode production",
    "deploy": "gh-pages -d examples/dist",
    "publish-demo": "npm run build && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sanusart/react-dropdown-select"
  },
  "keywords": [
    "react",
    "component",
    "select",
    "dropdown"
  ],
  "author": "Sasha Khamkov <sanusart@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": ">= 16.3.0",
    "react-dom": ">= 16.3.0",
    "prop-types": ">= 15.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.5",
    "babel-plugin-transform-dynamic-import": "^2.1.0",
    "babel-preset-minify": "^0.5.0",
    "coveralls": "^3.0.2",
    "css-loader": "^2.0.2",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "prettier": "1.15.3",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-test-renderer": "^16.7.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "normalize.css": "^8.0.1",
    "react-onclickout": "^2.0.8"
  },
  "jest": {
    "transform": {
      "^.+\\.(js)$": "babel-jest"
    },
    "testURL": "http://localhost/",
    "collectCoverage": false,
    "bail": true,
    "roots": [
      "<rootDir>/__tests__"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!node_modules/**",
      "!docs/**",
      "!examples/**"
    ]
  }
}
